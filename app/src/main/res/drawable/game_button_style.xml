<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <corners
                android:radius="12dp"/>
            <padding
                android:left="6dp"
                android:top="6dp"
                android:right="6dp"
                android:bottom="6dp"/>
            <stroke
                android:width="2dp"
                android:color="@color/black"/>
            <solid
                android:color="@color/white"/>
        </shape>
    </item>
    <item
        android:id="@+id/game_button_color">
        <shape
            android:shape="rectangle">
            <corners
                android:radius="12dp"/>
            <solid
                android:color="#898B89"/>
        </shape>
    </item>

</layer-list>